{
    "oidcGrantToken": {
        "gateway": "uuidentity.plus4u.net",
        "uuAppName": "uu-oidc-maing02",
        "awid": "xxx",
        "uuCmd": "oidc/grantToken",
        "tokenPath": "./oidc-token"
    },
    "uuThing": {
        "accessCode1": "xxx",
        "accessCode2": "xxx"
    },
    "uuApp": {
        "gateway": "uuapp.plus4u.net",
        "uuAppName": "uu-console-maing02",
        "awid": "xxx",
        "uuCmdList": {
            "guardManProgressGet": "progress/get"
        }
    },
    "gateway": {
        "moduleTimers": {
            "guardMan": 2
        },
        "guardMan": {
            "ledStrip": {
                "virtualMode": false,
                "pixels": 8,
                "pin": "D10"
            },
            "progressCode": "uuConsoleProgressExample003_6x11x1",
            "special": {
                "error": {
                    "color": "#ff0000",  
                    "action": "blink",
                    "interval": 0.1
                }
            },
            "progress": {
                "ledMinPosition": 0,
                "ledMaxPosition": 1,
                "waiting": {
                    "color": "#0000ff",
                    "action": "solid",
                    "interval": null
                },
                "started": {
                    "color": "#00ff00",
                    "action": "solid",
                    "interval": null
                },
                "running": {
                    "color": "#00ff00",
                    "action": "blink",
                    "interval": 0.2
                },
                "runningWithWarning": {
                    "color": "#ff8700",
                    "action": "blink",
                    "interval": 0.2
                },
                "runningWithError": {
                    "color": "#ff0000",
                    "action": "blink",
                    "interval": 0.2
                },
                "completed": {
                    "color": "#ffffff",
                    "action": "solid",
                    "interval": null
                },
                "completedWithWarning": {
                    "color": "#ff8700",
                    "action": "solid",
                    "interval": null
                },
                "completedWithError": {
                    "color": "#ff0000",
                    "action": "solid",
                    "interval": null
                },
                "error": {
                    "color": "#ff0000",  
                    "action": "solid",
                    "interval": null
                },
                "warning": {
                    "color": "#ff8700",
                    "action": "solid",
                    "interval": null
                }
            },
            "delay": {
                "ledMinPosition": 3,
                "ledMaxPosition": 4,
                "noDelay": {
                    "color": "#00ff00",
                    "action": "solid",
                    "interval": null
                },
                "delayedWithWarning": {
                    "color": "#ff8700",
                    "action": "solid",
                    "interval": null
                },
                "delayedWithError": {
                    "color": "#ff0000",
                    "action": "solid",
                    "interval": null
                }
            }, 
            "defaultDelays": {
                "warning": 3,
                "error": 10
            },
            "connection": {
                "ledMinPosition": 6,
                "ledMaxPosition": 7,
                "connected": {
                    "color": "#00ff00",
                    "action": "solid",
                    "interval": null
                },
                "noConnection": {
                    "color": "#ff0000",
                    "action": "solid",
                    "interval": null
                }
            }
        }
    }
}
