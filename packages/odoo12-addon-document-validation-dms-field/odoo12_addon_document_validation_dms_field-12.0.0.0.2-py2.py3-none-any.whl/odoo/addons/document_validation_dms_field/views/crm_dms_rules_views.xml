<odoo>
    <data>
        <record id="crm_dms_stage_team_rules_tree_view" model="ir.ui.view">
            <field name="name">crm.dms.stage.team.tree</field>
            <field name="model">crm.dms.stage.team.rules</field>
            <field name="arch" type="xml">
                <tree editable="top">
                    <field name="crm_stage_id" />
                    <field name="crm_team_id"/>
                    <field name="dms_category_id"/>
                    <field name="is_valid"/>
                    <field name="is_warning"/>
                    <field name="is_block"/>
                </tree>
            </field>
        </record>

        <record id="crm_dms_stage_team_rules_action_all" model="ir.actions.act_window">
            <field name="name">Crm Dms Stage Team Rules</field>
            <field name="res_model">crm.dms.stage.team.rules</field>
            <field name="view_mode">tree,kanban,form</field>
            <field name="view_id" ref="crm_dms_stage_team_rules_tree_view" />
        </record>
    </data>
</odoo>
