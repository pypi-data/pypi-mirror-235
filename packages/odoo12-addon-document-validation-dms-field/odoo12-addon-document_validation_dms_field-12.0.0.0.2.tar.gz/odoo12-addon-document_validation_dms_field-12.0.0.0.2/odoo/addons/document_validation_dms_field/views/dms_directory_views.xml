<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="inherit_view_dms_directory_form" model="ir.ui.view">
        <field name="name">dms_directory.form</field>
        <field name="model">dms.directory</field>
        <field name="inherit_id" ref="dms.view_dms_directory_form"/>
        <field name="arch" type="xml">
            <xpath expr="//sheet/group[@name='data']" position="inside">
                <group>
                    <field name="expiry_date" widget="date"/>
                    <field name="code"/>
                </group>
            </xpath>
        </field>
    </record>
</odoo>
